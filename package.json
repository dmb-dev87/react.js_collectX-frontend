{
  "name": "collectx",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-scss": "node-sass src/assets/scss/collectx.scss src/assets/css/collectx.css",
    "minify-sass": "node-sass src/assets/scss/collectx.scss src/assets/css/collectx.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/collectx.scss src/assets/css/collectx.css --source-map true"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "optionalDependencies": {
    "@types/googlemaps": "3.39.3",
    "@types/markerclustererplus": "2.1.33",
    "eslint-plugin-flowtype": "3.13.0",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@types/react": "16.9.23",
    "axios": "^0.19.2",
    "chart.js": "2.9.3",
    "classnames": "2.2.6",
    "history": "4.10.1",
    "node-sass": "4.13.1",
    "perfect-scrollbar": "1.5.0",
    "prop-types": "15.7.2",
    "rc-slider": "^9.3.1",
    "react": "16.13.0",
    "react-chartjs-2": "2.9.0",
    "react-dom": "16.13.0",
    "react-google-maps": "9.4.5",
    "react-loader-spinner": "^3.1.14",
    "react-notification-alert": "0.0.12",
    "react-redux": "^7.2.0",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.4.0",
    "react-tabs": "^3.1.1",
    "reactstrap": "8.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {}
}
